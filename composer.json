        {
            "name": "Andreas Mischke",
            "email": "andreas@mischke.me"
        }
    ],
    "require": {
        "hidehalo/nanoid-php": "^1.1",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "respect/validation": "^2.2",
